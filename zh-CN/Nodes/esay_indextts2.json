        },
        "widgets": {
            "text": "合成文本",
            "unload_model": "合成后卸载模型",
            "seed": "随机种子"
        },
        "outputs": {
            "audio": "音频",
            "seed": "随机种子",
            "subtitle": "字幕"
        }
    },
