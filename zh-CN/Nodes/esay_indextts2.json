            "model": "模型",
            "download_from": "下载来源",
            "start": "开始"
        },
        "outputs": {
            "indextts_model": "IndexTTS模型"
        }
    },
    "easy indexTTSGenerate": {
        "title": "IndexTTS语音生成",
        "description": "基于参考音频和情感配置生成语音，支持单音色和多音色合成",
        "inputs": {
            "indextts_model": "IndexTTS模型",
            "reference_audio": "参考音频(单音色)",
            "reference_audios": "参考音频(多音色)",
            "emotions": "情感配置"
        },
        "widgets": {
            "text": "合成文本",
            "unload_model": "合成后卸载模型",
            "do_sample": "采样生成",
            "temperature": "温度系数",
            "top_p": "核采样阈值",
            "top_k": "Top-K采样",
            "num_beams": "波束搜索数",
            "repetition_penalty": "重复惩罚系数",
            "length_penalty": "长度惩罚系数",
            "max_mel_tokens": "最大Mel tokens数",
            "max_tokens_per_sentence": "每句最大tokens数",
            "seed": "随机种子"
        },
        "outputs": {
            "audio": "音频",
            "seed": "随机种子",
            "subtitle": "字幕"
        }
    },
    "easy indexTTSGenerateSimple": {
